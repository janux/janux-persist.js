{
	"defaultSeverity": "off",
	"extends": [
		"tslint:recommended"
	],
	"jsRules": {},
	"rules": {
		"eofline": false,
		"quotemark": [
			false
		],
		"variable-name": [
			true,
			"ban-keywords",
			"check-format",
			"allow-leading-underscore"
		],
		"trailing-comma": [
			false
		],
		"object-literal-sort-keys": [
			false
		],
		"max-line-length": [
			false
		],
		"curly": false,
		"member-access": [
			false,
			"check-accessor",
			"check-constructor"
		],
		"no-empty-interface": false,
		"no-console": true,
		"no-shadowed-variable": false,
		"interface-name": [
			false
		],
		"no-string-literal": false,
		"indent": [
			true,
			"tabs"
		],
		"no-empty": false,
		"arrow-parens": [
			true,
			"ban-single-arg-parens"
		],
		"align": false
	},
	"rulesDirectory": []
}
